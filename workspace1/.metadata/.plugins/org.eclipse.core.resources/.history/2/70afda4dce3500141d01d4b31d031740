package it.gm.datastructure.graphs.shortestpath.model;

import java.util.List;

import it.gm.datastructure.graphs.core.model.Edge;
import it.gm.datastructure.graphs.core.model.Vertex;

public class VertexSP extends Vertex {
	
	public VertexSP(String name, List<Edge> adj, double dist, Vertex prev) {
		super(name, adj);
		this.dist=dist;
		this.prev=prev;
	}
	private double dist;
	private Vertex prev;

}
