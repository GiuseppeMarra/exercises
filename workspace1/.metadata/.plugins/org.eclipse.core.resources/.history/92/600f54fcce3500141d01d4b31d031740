package it.gm.datastructure.graphs.core.model;

import java.util.List;

public class Vertex {
	
	private String name;  //vertex name
	private List<Edge> adj;  //adjacency list
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public List<Edge> getAdj() {
		return adj;
	}
	public void setAdj(List<Edge> adj) {
		this.adj = adj;
	}
	public Vertex(String name, List<Edge> adj) {
		super();
		this.name = name;
		this.adj = adj;
	}
	
	

}
